{
  "name": "react-fixed-sidebar",
  "version": "1.1.1",
  "description": "A fixed sidebar for react apps including support for mobile touch to open/close",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve:hot",
    "build": "webpack",
    "build:lib": "webpack --config webpack.lib.config.js -p",
    "watch": "webpack --watch",
    "serve": "webpack-dev-server",
    "serve:hot": "webpack-dev-server --inline --hot -d",
    "clean": "rm -rf dist/ lib/",
    "prepublish": "npm run build:lib"
  },
  "author": "James Batt",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "node-sass": "^3.8.0",
    "path": "^0.12.7",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "hammerjs": "^2.0.8",
    "react": "^15.0.1",
    "react-dom": "^15.0.1"
  }
}
